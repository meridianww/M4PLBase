@using System.Web.UI.WebControls;
@model M4PL.Entities.Menus

@*Start Menu Driver Date Changed And Entered*@

<div class="row">
    <div class="col-lg-6 col-md-6 col-sm-6 form-group">
        <label>@Model.LblMnuDateEntered</label>
        @Html.DevExpress().TextBoxFor(m => m.MnuDateEntered,
        settings =>
        {
            settings.Properties.NullText = "Menu Date Entered";
            settings.ControlStyle.CssClass = "form-control readonlyBgColor";
            settings.Width = Unit.Percentage(100);
            settings.Properties.MaxLength = 25;
        }).GetHtml()
    </div>
    <div class="col-lg-6 col-md-6 col-sm-6 form-group">
        <label>@Model.LblMnuDateChanged</label>
        @Html.DevExpress().TextBoxFor(m => m.MnuDateChanged,
        settings =>
        {
            settings.Properties.NullText = "Menu Date Changed";
            settings.ControlStyle.CssClass = "form-control readonlyBgColor";
            settings.Width = Unit.Percentage(100);
            settings.Properties.MaxLength = 25;
        }).GetHtml()
    </div>
</div>
<div class="row">
    <div class="col-lg-6 col-md-6 col-sm-6 form-group">
        <label>@Model.LblMnyDateEnteredBy</label>
        @Html.DevExpress().TextBoxFor(m => m.MnuEnteredBy,
        settings =>
        {
            settings.Properties.NullText = "Menu Entered By";
            settings.ControlStyle.CssClass = "form-control readonlyBgColor";
            settings.Width = Unit.Percentage(100);
            settings.Properties.MaxLength = 25;
        }).GetHtml()
    </div>
    <div class="col-lg-6 col-md-6 col-sm-6 form-group">
        <label>@Model.LblMnuDateChangedBy</label>
        @Html.DevExpress().TextBoxFor(m => m.MnuChangedBy,
        settings =>
        {
            settings.Properties.NullText = "Menu Changed By";
            settings.ControlStyle.CssClass = "form-control readonlyBgColor";
            settings.Width = Unit.Percentage(100);
            settings.Properties.MaxLength = 25;
        }).GetHtml()
    </div>
</div>

@*End Menu Driver Date Changed And Entered*@