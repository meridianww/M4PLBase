@using System.Web.UI.WebControls
@using M4PL.Entities
@model  M4PL.Web.Models.GridResult<M4PL.Entities.MasterTables.ChooseColumn>
@{
    var currentStrRoute = Newtonsoft.Json.JsonConvert.SerializeObject(Model.GridSetting.CallBackRoute);
}
@using (Html.BeginForm(MvcConstants.InsAndUpdChooseColumn, Model.GridSetting.CallBackRoute.Controller, FormMethod.Post, new { id = WebApplicationConstants.ChooseColumnFormId, @class = "col-lg-12 col-md-12" }))
{
    <div>
        @Html.Action("UpdateChooseColumn", EntitiesAlias.Common.ToString(), new { Area = string.Empty, selectedColumns = string.Empty, currentOperation = string.Empty, strRoute = currentStrRoute, availableColumnLastIndex = 0 })
    </div>
}