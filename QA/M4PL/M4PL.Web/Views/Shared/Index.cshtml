@model M4PL.Entities.Support.MvcRoute
@{
    Layout = "~/Views/Shared/_ContentAreaLayout.cshtml";
}
@Html.Action("CallbackPanelPartial", new { strRoute = Newtonsoft.Json.JsonConvert.SerializeObject(Model) })
