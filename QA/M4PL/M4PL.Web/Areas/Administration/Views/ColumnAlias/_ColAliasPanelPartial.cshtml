@model M4PL.Web.Models.GridResult<M4PL.APIClient.ViewModels.Administration.ColumnAliasView>

@Html.DevExpress().CallbackPanel(settings =>
{
    settings.Name = "ColAliasCbPanel";
    settings.CallbackRouteValues = new { Controller = Model.GridSetting.CallBackRoute.Controller, Action = "DataViewCallback", Area = Model.GridSetting.CallBackRoute.Area, strRoute = Newtonsoft.Json.JsonConvert.SerializeObject(Model.GridSetting.CallBackRoute) };

    settings.SetContent(() =>
    {
        Html.RenderPartial(MvcConstants.GridViewPartial, Model);
    });

}).GetHtml()